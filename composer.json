{
  "name": "officeutils/phpactiverecordbundle",
  "type": "symfony-bundle",
  "description": "The \"Symfony 2\" bundle for easy use PHPActiveRecord library in project",
  "license": "MIT",
  "autoload": {
    "psr-4": { "": "src/" }
  },
  "require": {
    "phpactiverecord": ">=1.0.0",
    "pear/log": "1.12.9"
  },
  "repositories":[
    {
      "type":"package",
      "package":{
        "name":"phpactiverecord",
        "version":"1.1.0",
        "source" : {
          "type":"git",
          "url":"https://github.com/jpfuentes2/php-activerecord/",
          "reference":"master"
        },
        "autoload":{
          "files":["ActiveRecord.php"]
        }
      }
    }
 ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.0-dev"
    }
  }
}